<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">



    <EditText
        android:id="@+id/Entry"
        android:layout_width="143dp"
        android:layout_height="45dp"
        android:layout_marginStart="28dp"
        android:ems="10"
        android:inputType="number"
        android:hint="Enter Zip"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="46dp" />

    <Button
        android:id="@+id/Send"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="56dp"
        android:text="Send"
        app:layout_constraintStart_toEndOf="@+id/Entry"
        tools:layout_editor_absoluteY="46dp" />

    <Button
        android:id="@+id/ZipOne"
        android:layout_width="154dp"
        android:layout_height="51dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="92dp"
        android:text="Zip One"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Entry" />

    <Button
        android:id="@+id/ZipThree"
        android:layout_width="154dp"
        android:layout_height="51dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="60dp"
        android:text="Zip Three"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ZipOne" />

    <Button
        android:id="@+id/ZipFour"
        android:layout_width="154dp"
        android:layout_height="51dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="60dp"
        android:text="Zip Four"
        app:layout_constraintStart_toEndOf="@+id/ZipThree"
        app:layout_constraintTop_toBottomOf="@+id/ZipTwo" />

    <Button
        android:id="@+id/ZipTwo"
        android:layout_width="154dp"
        android:layout_height="51dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="88dp"
        android:text="Zip Two"
        app:layout_constraintStart_toEndOf="@+id/ZipOne"
        app:layout_constraintTop_toBottomOf="@+id/Send" />

</androidx.constraintlayout.widget.ConstraintLayout>